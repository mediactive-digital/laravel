{
    "name": "mediactive-digital/laravel",
    "description": "Reliese Components for Laravel Framework code generation.",
    "keywords": ["reliese", "laravel"],
    "support": {
        "issues": "https://github.com/mediactive-digital/laravel/issues",
        "source": "https://github.com/mediactive-digital/laravel"
    },
    "license": "MIT",
    "authors": [{
		"name": "Mediactive Digital"
	}],
    "require": {
        "php": ">=7",
        "doctrine/dbal": "~2.5",
        "illuminate/support": "^5.5|^6",
        "illuminate/database": "^5.5|^6",
        "illuminate/contracts": "^5.5|^6",
        "illuminate/filesystem": "^5.5|^6",
        "illuminate/console": "^5.5|^6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Reliese\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reliese\\Coders\\CodersServiceProvider"
            ]
        }
    }
}
